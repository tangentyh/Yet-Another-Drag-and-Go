{
	"manifest_version" : 3,
	"name" : "Yet Another Drag and Go",
	"short_name" : "yadng",
	"version" : "0.3.3",
	"default_locale" : "en",
	"description" : "__MSG_desc__",
	"icons" : {
		"16" : "img/yadng_16.png",
		"48" : "img/yadng_48.png",
		"128" : "img/yadng_128.png"
	},
	"author" : "hr6r",
	"background" : {
		"service_worker" : "background.js"
	},
	"content_scripts" : [{
				"matches" : ["http://*/*", "https://*/*", "ftp://*/*"],
				"js" : ["common.js", "content.js"],
				"run_at" : "document_start"
			}],
	"permissions" : ["storage", "tabs"],
	"host_permissions" : ["http://*/*", "https://*/*", "ftp://*/*"],
	"options_page" : "options.html"
}